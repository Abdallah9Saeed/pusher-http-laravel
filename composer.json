{
	"name": "vinkla/pusher",
	"description": "Laravel wrapper for the Pusher HTTP API.",
	"license": "MIT",
	"keywords": ["laravel", "pusher", "http", "api", "php-pusher-server", "rest", "realtime", "trigger", "publish", "events"],
	"authors": [
		{
			"name": "Vincent Klaiber",
			"email": "hello@vinkla.com"
		}
	],
	"require": {
		"php": ">=5.4.7",
		"illuminate/contracts": "~5.0",
		"illuminate/support": "~5.0",
		"graham-campbell/manager": "~2.0",
		"pusher/pusher-php-server": "~2.0"
	},
	"require-dev": {
		"graham-campbell/testbench": "~2.0"
	},
	"autoload": {
		"psr-4": {
			"Vinkla\\Pusher\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Vinkla\\Tests\\Pusher\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
